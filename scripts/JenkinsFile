timestamps {

  node () {

	stage ('Checkout') {
 	  checkout([$class: 'GitSCM', branches: [[name: '**']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[credentialsId: '', url: 'https://github.com/emccrory/ddisplay.git']]]) 
	}

	stage ('Validate') {
		sh """
		cd DynamicDisplays
		mvn -B -DskipTests clean validate
		"""
	}
	
	stage ('Build') {
		sh """
		cd DynamicDisplays
		mvn -B -DskipTests clean compile
		""" 
	}
	
	stage ('Unit Tests') {
		sh """
		cd DynamicDisplays
		mvn -B test
		"""
	}
	
	stage ('Rudimentary Run Test Linux') {
		sh """
		cd DynamicDisplays
		. ./setupJars.sh
		java gov.fnal.ppd.dd.util.nonguiUtils.JavaVersion
		"""
	}
  }
 }